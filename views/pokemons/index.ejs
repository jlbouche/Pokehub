<%- include('../partials/header') %>

<body>
    <table>
        <thead>
            <tr>
                <th>Pokemon</th>
                <th>Pokemon ID</th>
                <th>Pokemon Type 1</th>
                <th>Pokemon Type 2</th>
            </tr>
        </thead>
        <tbody>
            <% pokemons.forEach(function(p) { %>
                <tr>
                    <td><%= p.pokename %></td>
                    <td><%= p.pokeid %></td>
                    <td><%= p.poketype1 %></td>
                    <td><%= p.poketype2 %></td>
                    <td><a href="/pokemons/<%=p._id %>">Details/Researcher Notes</a></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>

<%- include('../partials/footer') %>